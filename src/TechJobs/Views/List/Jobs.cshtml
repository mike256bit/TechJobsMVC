<h1>@ViewBag.title</h1>

@if (ViewBag.jobs != null)
{

    <h3>@ViewBag.jobs.Count Result(s)</h3>

    @foreach (Dictionary<string, string> aJob in ViewBag.jobs)
    {
        <table class="job-listing">
            @foreach (KeyValuePair<string, string> jobDetails in aJob)
            {
                <tr>
                    <td>@jobDetails.Key</td>
                    <td>@jobDetails.Value</td>
                </tr>
            }
        </table>
    }
}
